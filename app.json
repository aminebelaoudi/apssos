          "mapboxPublicKey": "pk.eyJ1IjoiYW1pbmViZWxhb3VkaSIsImEiOiJjbTlkNjUxdHgwcTk1MmtxcXFwbHVma3h1In0.GENafHT2oSBRbRhi4z9N2g"
